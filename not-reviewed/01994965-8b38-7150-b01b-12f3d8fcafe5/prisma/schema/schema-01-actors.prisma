/// Unique email per end user. Supports soft deletion and timestamps for
/// auditing user record changes.
///
/// @namespace Actors
/// @author AutoBE - https://github.com/wrtnlabs/autobe
model telegram_file_downloader_endusers {
  //----
  // COLUMNS
  //----
  /// Primary Key.
  id String @id
  
  /// Email address of the end user, unique across end users.
  email String
  
  /// Hashed password for authenticating the end user.
  password_hash String
  
  /// Timestamp when the end user record was created.
  created_at DateTime
  
  /// Timestamp when the end user record was last updated.
  updated_at DateTime
  
